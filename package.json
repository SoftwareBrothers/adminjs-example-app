{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "typeorm": "./node_modules/typeorm/cli.js --dataSource dist/sources/typeorm/config.js",
    "sequelize": "yarn sequelize-cli --config src/sources/sequelize/config.js",
    "migration:up": "yarn sequelize db:migrate && yarn typeorm migration:run && yarn mikro-orm migration:up && yarn prisma migrate dev --schema prisma/schema.prisma",
    "migration:up:production": "yarn sequelize db:migrate && yarn typeorm migration:run && yarn mikro-orm migration:up && yarn prisma migrate deploy --schema prisma/schema.prisma",
    "docker:up": "docker-compose -f docker-compose.yaml up -d",
    "start:dev": "nodemon",
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc",
    "build:watch": "rm -rf dist && tsc -w",
    "postgres:clean": "node dist/scripts/truncate-postgres",
    "mysql:clean": "prisma migrate reset --force && prisma migrate deploy",
    "mongodb:clean": "node dist/scripts/truncate-mongodb",
    "db:clean": "yarn postgres:clean && yarn mysql:clean && yarn mongodb:clean",
    "seed:typeorm": "node dist/sources/typeorm/seeds/run",
    "seed:sequelize": "node dist/sources/sequelize/seeds/run",
    "seed:mongoose": "node dist/sources/mongoose/seeds/run",
    "seed:mikro-orm": "node dist/sources/mikroorm/seeds/run",
    "seed:prisma": "node dist/sources/prisma/seeds/run",
    "seed:all": "yarn seed:typeorm && yarn seed:sequelize && yarn seed:mongoose && yarn seed:mikro-orm && yarn seed:prisma"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/sources/mikroorm/config.js",
      "./src/sources/mikroorm/config.ts"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@adminjs/design-system": "^3.0.0",
    "@adminjs/express": "^5.0.0",
    "@adminjs/fastify": "^2.0.0",
    "@adminjs/hapi": "^6.0.0",
    "@adminjs/mikroorm": "^2.0.0",
    "@adminjs/mongoose": "^3.0.0",
    "@adminjs/passwords": "^3.0.0",
    "@adminjs/prisma": "^2.0.0",
    "@adminjs/sequelize": "^3.0.0",
    "@adminjs/typeorm": "^4.0.0",
    "@faker-js/faker": "^7.3.0",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@hapi/boom": "^9.1.4",
    "@hapi/cookie": "^11.0.2",
    "@hapi/hapi": "^20.2.1",
    "@mikro-orm/cli": "^4",
    "@mikro-orm/core": "^4",
    "@mikro-orm/migrations": "^4",
    "@mikro-orm/nestjs": "^4",
    "@mikro-orm/postgresql": "^4",
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^8.3.1",
    "@nestjs/typeorm": "^9.0.0",
    "@prisma/client": "^3.9.2",
    "adminjs": "^6.0.1",
    "argon2": "^0.28.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-pg-simple": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "fastify-static": "^4.5.0",
    "mongoose": "^6.2.1",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "prisma": "^3.12.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.5.4",
    "sequelize": "^6.16.1",
    "sequelize-cli": "^6.4.1",
    "styled-components": "^5.3.5",
    "tslib": "^2.3.1",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.10",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
