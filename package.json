  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/sources/typeorm/config.ts",
    "sequelize": "yarn sequelize-cli --config src/sources/sequelize/config.js",
    "migration:up": "yarn sequelize db:migrate && yarn mikro-orm migration:up && yarn prisma migrate dev --schema src/sources/prisma/schema.prisma",
    "docker:up": "docker-compose -f docker-compose.yaml up -d",
    "start:dev": "nodemon --watch src",
    "build": "tsc"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/sources/mikroorm/config.ts"
    ]
  },
